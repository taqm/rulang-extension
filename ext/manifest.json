{
  "manifest_version": 2,
  "name": "ルー語翻訳",
  "description": "選択範囲をルー語にその場で翻訳",
  "version": "1.0.1",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["jquery-3.2.1.min.js", "content.js"],
      "css": ["content.css"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "contextMenus"
  ]
}
